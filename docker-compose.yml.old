version: '3'
services:
  frontend:
    build:
      context: ./frontend
      dockerfile: Dockerfile
      args:
        VITE_GRAFANA_URL: http://mrxprt-vm/grafana
        VITE_GRAFANA_DEFAULT_ORG_ID: 1
        ARG VITE_AUTH_URL
        ARG VITE_AUTH_CLIENT_ID
        VITE_AUTH_CLIENT_SECRET: 
        VITE_AUTH_REDIRECT_URI: 
    restart: unless-stopped
    expose: 
      - "5173"
